<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MMCalendar">
        <attr name="dates_from_current_month_text_color" format="color" />
        <attr name="should_show_calendar_events" format="boolean" />
        <attr name="calendar_name" format="string" />
        <attr name="dates_from_current_month_background_drawable" format="reference" />
        <attr name="dates_from_next_month_text_color" format="color" />
        <attr name="dates_from_previous_month_text_color" format="color" />
        <attr name="dates_from_current_month_text_size" format="dimension" />
        <attr name="dates_from_next_month_text_size" format="dimension" />
        <attr name="dates_from_previous_month_text_size" format="dimension" />
        <attr name="month_year_text_size" format="dimension" />
        <attr name="month_year_text_color" format="color" />
        <attr name="current_date_text_color" format="color" />
        <attr name="current_date_text_size" format="dimension" />
        <attr name="current_date_background_drawable" format="reference" />
        <attr name="previous_month_dates_background_drawable" format="reference" />
        <attr name="next_month_dates_background_drawable" format="reference" />
        <attr name="title_text_gravity" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
            <enum name="center" value="2"/>
        </attr>
    </declare-styleable>
</resources>